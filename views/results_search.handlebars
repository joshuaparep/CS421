<!--jumbotron-->
<div class="jumbotron">
	<div class="container text-center">
		<h1>Hele-On Bus</h1>
		<p>Hawaii County Mass Transit Agency</p>
	</div><!--end container-->
</div><!--end jumbotron-->

<!--Gallery-->
<div class="#" id="image">

	<section>
				
		<div id="container" align="center" class="relative">
		
			<div class="absolute-text">
				<div class="w3-container w3-red ">
				<h2><strong>{{title}}</strong></h2>
				<h4><strong>Search Results from 
					{{#if query.loc_start}}{{query.loc_start}}{{else}}(Any start location){{/if}} 						 to {{#if query.loc_end}}{{query.loc_end}}{{else}}(Any end location){{/if}}
					 on {{#if query.day}}{{query.day}}{{else}}(Any day){{/if}}
					 between {{#if query.time_start}}{{query.time_start}}{{else}}(Any start time){{/if}}
					 and {{#if query.time_end}}{{query.time_end}}{{else}}(Any end time){{/if}}
				</strong></h4>
				<br><br>				 
       			</div>
       			<br><br><br>
				<div class="container text-center" text-align="center">
					<strong>
					{{#each table}}
					
					<table align="center" border="1px">
					
					<tr>
					<th colspan="100" class="table_title">{{this.R_Name}}</td>
					</tr>
					<tr class='location_name'>
					{{#each locations}}
					<td class="{{this.Chosen}}"><span class="l_name">{{this.L_Name}}</span></td>
					{{/each}}
					</tr>
					<tr class='location_time'>
					{{#each locations}}
					<td class="{{this.Chosen}}"><time class="l_time">{{this.TIME_12}}</time></td>
					{{/each}}
					</tr>
					</table>
					
					{{else}}

					<p>No Results Found!</p>

					{{/each}}
					</strong>
				</div><!--end container-->
			</div><!--end container-->

		</div>			
	</section>
</div> <!--carousel end -->
